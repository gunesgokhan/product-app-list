{
  "version": 2,
  "builds": [
    {
      "src": "product-app-list/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "build"
      }
    },
    {
      "src": "product-app-list/backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/product-app-list/backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/product-app-list/frontend/build/$1"
    }
  ],
  "installCommand": "npm install --prefix product-app-list/frontend && npm install --prefix product-app-list/backend",
  "buildCommand": "npm run build --prefix product-app-list/frontend",
  "outputDirectory": "product-app-list/frontend/build",
  
}
